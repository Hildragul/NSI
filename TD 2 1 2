nbm_1 = int(input('Entrez un premier nombre :'))
nbm_2 = int(input('Entrez un second nombre :'))

if nbm_1 < nbm_2:
    print('Le deuxieme nombre est plus grand')
else:
    print("Le premier nombre est plus grand")



